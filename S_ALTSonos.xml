<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
    <specVersion>
        <major>1</major>
        <minor>0</minor>
    </specVersion>
    <serviceStateTable>
        <stateVariable sendEvents="no">
            <name>Debug</name>
            <dataType>boolean</dataType>
            <defaultValue>0</defaultValue>
            <shortCode>debug</shortCode>
        </stateVariable>
        <stateVariable sendEvents="no">
            <name>groupID</name>
            <dataType>string</dataType>
            <defaultValue></defaultValue>
            <shortCode>groupid</shortCode>
        </stateVariable>
        <stateVariable sendEvents="no">
            <name>favID</name>
            <dataType>string</dataType>
            <defaultValue></defaultValue>
            <shortCode>favid</shortCode>
        </stateVariable>
        <stateVariable sendEvents="no">
            <name>volumeDelta</name>
            <dataType>string</dataType>
            <defaultValue></defaultValue>
            <shortCode>volumedelta</shortCode>
        </stateVariable>	
        <stateVariable sendEvents="no">
            <name>LastVolume</name>
            <dataType>string</dataType>
            <defaultValue></defaultValue>
            <shortCode>lastVolume</shortCode>
        </stateVariable>	
        <stateVariable sendEvents="no">
            <name>playerID</name>
            <dataType>string</dataType>
            <defaultValue></defaultValue>
            <shortCode>playerid</shortCode>
        </stateVariable>	
        <stateVariable sendEvents="no">
            <name>urlClip</name>
            <dataType>string</dataType>
            <defaultValue></defaultValue>
            <shortCode>urlclip</shortCode>
        </stateVariable>	
        <stateVariable sendEvents="no">
            <name>duration</name>
            <dataType>ui4</dataType>
            <defaultValue></defaultValue>
            <shortCode>duration</shortCode>
        </stateVariable>	
	</serviceStateTable>
    <actionList>
        <action>
            <name>SetDebug</name>
            <argumentList>
                <argument>
                    <name>newDebugMode</name>
                    <direction>in</direction>
                    <relatedStateVariable>Debug</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>Discover</name>
        </action>
        <action>
            <name>GetVolume</name>
            <argumentList>
				<argument>
					<name>groupID</name>
					<direction>in</direction>
					<relatedStateVariable>groupid</relatedStateVariable>
				</argument>
				<argument>
					<name>LastVolume</name>
					<direction>out</direction>
					<relatedStateVariable>LastVolume</relatedStateVariable>
				</argument>
            </argumentList>
        </action>
		<action>
			<name>SetVolumeRelative</name>
            <argumentList>
				<argument>
					<name>groupID</name>
					<direction>in</direction>
					<relatedStateVariable>groupid</relatedStateVariable>
				</argument>
				<argument>
					<name>volumeDelta</name>
					<direction>in</direction>
					<relatedStateVariable>volumeDelta</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
        <action>
            <name>Play</name>
            <argumentList>
			<argument>
				<name>groupID</name>
				<direction>in</direction>
				<relatedStateVariable>groupid</relatedStateVariable>
			</argument>
            </argumentList>
        </action>
        <action>
            <name>Pause</name>
            <argumentList>
			<argument>
				<name>groupID</name>
				<direction>in</direction>
				<relatedStateVariable>groupid</relatedStateVariable>
			</argument>
            </argumentList>
        </action>
        <action>
            <name>Prev</name>
            <argumentList>
			<argument>
				<name>groupID</name>
				<direction>in</direction>
				<relatedStateVariable>groupid</relatedStateVariable>
			</argument>
            </argumentList>
        </action>
		<action>
            <name>Next</name>
            <argumentList>
			<argument>
				<name>groupID</name>
				<direction>in</direction>
				<relatedStateVariable>groupid</relatedStateVariable>
			</argument>
            </argumentList>
        </action>
		<action>
			<name>LoadFavorite</name>
            <argumentList>
			<argument>
				<name>groupID</name>
				<direction>in</direction>
				<relatedStateVariable>groupid</relatedStateVariable>
			</argument>
			<argument>
				<name>favID</name>
				<direction>in</direction>
				<relatedStateVariable>favid</relatedStateVariable>
			</argument>
            </argumentList>
		</action>	
		<action>
			<name>AudioClip</name>
            <argumentList>
			<argument>
				<name>groupID</name>
				<direction>in</direction>
				<relatedStateVariable>groupid</relatedStateVariable>
			</argument>
			<argument>
				<name>urlClip</name>
				<direction>in</direction>
				<relatedStateVariable>urlclip</relatedStateVariable>
			</argument>
			<argument>
				<name>Duration</name>
				<direction>in</direction>
				<relatedStateVariable>duration</relatedStateVariable>
			</argument>
            </argumentList>
		</action>	
	</actionList>
</scpd>
