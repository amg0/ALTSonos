<?xml version="1.0"?>
<implementation>
  <functions>
  </functions>
  <files>L_ALTSonos.lua</files>
  <startup>initstatus</startup>
  <actionList>
		<action>
			<serviceId>urn:upnp-org:serviceId:altsonos1</serviceId>
			<name>SetDebug</name>
			<job>
			setDebugMode(lul_device,lul_settings.newDebugMode)
			return 4,0
			</job>
		</action>	         
		<action>
			<serviceId>urn:upnp-org:serviceId:altsonos1</serviceId>
			<name>Discover</name>
			<job>
			syncDevices(lul_device)
			return 4,0
			</job>
		</action>	         
		<action>
			<serviceId>urn:upnp-org:serviceId:altsonos1</serviceId>
			<name>Play</name>
			<job>
			groupPlayPause(lul_device,"play",lul_settings.groupID)
			return 4,0
			</job>
		</action>	         
		<action>
			<serviceId>urn:upnp-org:serviceId:altsonos1</serviceId>
			<name>Pause</name>
			<job>
			groupPlayPause(lul_device,"pause",lul_settings.groupID)
			return 4,0
			</job>
		</action>	         
		<action>
			<serviceId>urn:upnp-org:serviceId:altsonos1</serviceId>
			<name>Prev</name>
			<job>
			groupPlayPause(lul_device,"skipToPreviousTrack",lul_settings.groupID)
			return 4,0
			</job>
		</action>	
		<action>
			<serviceId>urn:upnp-org:serviceId:altsonos1</serviceId>
			<name>Next</name>
			<job>
			groupPlayPause(lul_device,"skipToNextTrack",lul_settings.groupID)
			return 4,0
			</job>
		</action>	
	</actionList>
</implementation>
